    <style>
        /* Content width constraint */
        .about-content-container {
            max-width: 950px !important;
            margin: 0 auto !important;
        }

        /* Button with circular icon */
        .btn-with-circle-icon {
            position: relative;
            padding: 18px 90px 18px 40px !important;
            overflow: visible !important;
            border: none !important;
            transition: all 0.3s ease;
            background-color: #E4F25A !important;
            color: #36556C !important;
        }

        .btn-with-circle-icon::before,
        .btn-with-circle-icon::after {
            display: none !important;
        }

        .btn-with-circle-icon .circle-icon {
            position: absolute;
            right: 6px;
            top: 50%;
            transform: translateY(-50%);
            width: 56px;
            height: 56px;
            background-color: #509ED8 !important;
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            transition: all 0.3s ease;
        }

        .btn-with-circle-icon .circle-icon::before,
        .btn-with-circle-icon .circle-icon::after {
            display: none !important;
        }

        .btn-with-circle-icon .circle-icon i {
            color: #ffffff !important;
            font-size: 1.4rem;
            margin-left: 2px;
        }

        .btn-with-circle-icon:hover {
            background-color: #E4F25A !important;
            transform: translateY(-2px);
            box-shadow: 0 6px 20px rgba(228, 242, 90, 0.4);
        }

        .btn-with-circle-icon:hover .circle-icon {
            background-color: #3d8bc4 !important;
            transform: translateY(-50%) translateX(5px);
        }

        /* Mobile devices (up to 576px) */
        @media (max-width: 575.98px) {
            .about-page-title { font-size: 2.5rem !important; }
            .about-section-title { font-size: 2rem !important; }
            .about-large-title { font-size: 2.2rem !important; }
            .about-body-text { font-size: 14px !important; }
            .about-subtitle { font-size: 0.9rem !important; }
            .breadcumb-wrapper { padding: 60px 0 !important; }
            .about-content-container { max-width: 100% !important; padding: 0 15px !important; }
        }

        /* Tablets (576px to 767.98px) */
        @media (min-width: 576px) and (max-width: 767.98px) {
            .about-page-title { font-size: 3rem !important; }
            .about-section-title { font-size: 2.5rem !important; }
            .about-large-title { font-size: 2.5rem !important; }
            .about-body-text { font-size: 15px !important; }
            .about-subtitle { font-size: 1rem !important; }
            .breadcumb-wrapper { padding: 80px 0 !important; }
            .about-content-container { max-width: 100% !important; padding: 0 15px !important; }
        }

        /* Small laptops (768px to 991.98px) */
        @media (min-width: 768px) and (max-width: 991.98px) {
            .about-page-title { font-size: 3.5rem !important; }
            .about-section-title { font-size: 3rem !important; }
            .about-large-title { font-size: 3rem !important; }
            .about-body-text { font-size: 16px !important; }
            .about-subtitle { font-size: 1.1rem !important; }
